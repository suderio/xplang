[0 1] + 1 = [1 2]
[0 1] + [1] = [1 1]
[0 1] + [1 2] = [1 3]

[0 1] < [5] = true
[0 1] < 5 = true

[1 2] & 1 = 2
3 & [1 2] = null
[1 2] | 1 = [1 2 2]
[1 2] & [] = [1 2]
[1 2] | [2] = [1 2 2]

"string" = ['s' 't' 'r' 'i' 'n' 'g']
's' + 1 = 't'
's' * 2 = ?

[1 2 3] & {1 2} = [2 3]
[1 2 3] & [1 2] = [1 2]
[1 2 3] & 2 = 3

[1 2 3] & {1 ...} = [2 3]
[1 2 3] & {... 1} = [1 2]
[1 2 3] & [2 ... 5] = [2 3]
[1 2 3] & 4 = null
[1] & [2 3] = []

[a: 1 b: 2 c: 4] & {a b} = [1 2]
[a: 1 b: 2 c: 4] & [a b] = []
[a: 1 b: 2 c: 4] & [1 2] = [a: 1 b: 2] ???

a: 1; b: 2; c: 4 = [a: 1 b: 2 c: 4]
'0; ...; '5 = {0 ... 5}